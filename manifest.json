{
  "name": "Verifica URL - Tibia",
  "description": "Verifica se a URL do tibia é a do site oficial.",
  "version": "0.1",
  "manifest_version": 3,
  "author": "Lário Diniz",
  "action":{
        "default_popup": "popup.html",
        "default_title": "Latest Covid Report"
    },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
  

}